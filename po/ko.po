#: daemon/gdm.c:144
#, c-format
msgid "gdm_config_parse: No configuration file: %s. Using defaults."
msgstr "gdm_config_parse: 설정 파일이 없습니다: %s. 기본값을 사용합니다."

#: daemon/gdm.c:218
msgid ""
"gdm_config_parse: Root cannot be autologged in, turing off automatic login"
msgstr ""
"gdm_config_parse: Root사용자는 자동로그인 할수 없습니다, 자동 로그인을 끕니다"

#: daemon/gdm.c:231
msgid "gdm_config_parse: Root cannot be autologged in, turing off timed login"
msgstr "gdm_config_parse: Root사용자는 자동로그인 할수 없습니다, 시간제한 로그인을 끕니다"

#: daemon/gdm.c:237
msgid "gdm_config_parse: TimedLoginDelay less then 10, so I will just use 10."
msgstr "gdm_config_parse: TimedLoginDelay 이 10보다 작습니다, 10을 사용합니다."

#: daemon/gdm.c:243
msgid "gdm_config_parse: No greeter specified."
msgstr "gdm_config_parse: greeter가 지정되어 있지 않았습니다."

#: daemon/gdm.c:247
msgid "gdm_config_parse: No authdir specified."
msgstr "gdm_config_parse: authdir가 지정되지 않습니다."

#: daemon/gdm.c:253
msgid "gdm_config_parse: No sessions directory specified."
msgstr "gdm_config_parse: 세션 디렉토리가 지정되지 않았습니다."

#: daemon/gdm.c:265
msgid "gdm_config_parse: Invalid server line in config file. Ignoring!"
msgstr "gdm_config_parse: 설정 파일에 X 서버 줄이 틀렸습니다. 무시!"

#: daemon/gdm.c:273
msgid ""
"gdm_config_parse: Xdmcp disabled and no local servers defined. Adding "
"/usr/bin/X11/X on :0 to allow configuration!"
msgstr ""
"gdm_config_parse: Xdmcp를 사용하지 않고 로칼 서버가 정의되지 않았습니다. "
"/usr/bin/X11/X 의 :0에 설정을 허용합니다."

#: daemon/gdm.c:285
msgid ""
"gdm_config_parse: Xdmcp disabled and no local servers defined. Aborting!"
msgstr ""
"gdm_config_parse: Xdmcp를 사용하지 않고 로칼 서버가 정의되지 않았습니다. "
"취소!"

#: daemon/gdm.c:294
#, c-format
msgid "gdm_config_parse: Can't find the gdm user (%s). Trying 'nobody'!"
msgstr "gdm_config_parse: gdm 사용자(%s)가 없습니다. 'nobody'로 시도합니다!"

#: daemon/gdm.c:301
#, c-format
msgid "gdm_config_parse: Can't find the gdm user (%s). Aborting!"
msgstr "gdm_config_parse: gdm 사용자(%s)가 없습니다. 중지!"

#: daemon/gdm.c:306
msgid "gdm_config_parse: The gdm user should not be root. Aborting!"
msgstr "gdm_config_parse: gdm 사용자는 루트이면 안 됩니다. 중지!"

#: daemon/gdm.c:311
#, c-format
msgid "gdm_config_parse: Can't find the gdm group (%s). Trying 'nobody'!"
msgstr "gdm_config_parse: gdm 그룹(%s)이 없습니다. 'nobody'로 시도합니다!"

#: daemon/gdm.c:318
#, c-format
msgid "gdm_config_parse: Can't find the gdm group (%s). Aborting!"
msgstr "gdm_config_parse: gdm 그룹(%s)이 없습니다. 중지!"

#: daemon/gdm.c:323
msgid "gdm_config_parse: The gdm group should not be root. Aborting!"
msgstr "gdm_config_parse: gdm 그룹은 루트이면 안 됩니다. 중지!"

#: daemon/gdm.c:359
#, c-format
msgid "gdm_config_parse: Authdir %s does not exist. Aborting."
msgstr "gdm_config_parse: Authdir %s이(가) 없습니다. 중지."

#: daemon/gdm.c:362
#, c-format
msgid "gdm_config_parse: Authdir %s is not a directory. Aborting."
msgstr "gdm_config_parse: Authdir %s은(는) 자료방이 아닙니다. 중지."

#: daemon/gdm.c:365
#, c-format
msgid ""
"gdm_config_parse: Authdir %s is not owned by user %s, group %s. Aborting."
msgstr ""
"gdm_config_parse: Authdir %s이(가) 사용자 %s, 그룹 %s의 소유가 아닙니다. "
"중지."

#: daemon/gdm.c:369
#, c-format
msgid ""
"gdm_config_parse: Authdir %s has wrong permissions. Should be 750. Aborting."
msgstr ""
"gdm_config_parse: Authdir %s의 접근권한이 틀렸습니다. 750이어야 합니다. 중지."

#: daemon/gdm.c:404
msgid "gdm_daemonify: fork() failed!"
msgstr "gdm_daemonify: fork() 실패!"

#: daemon/gdm.c:407
#, c-format
msgid "gdm_daemonify: setsid() failed: %s!"
msgstr "gdm_daemonify: setsid() 실패: %s!"

#: daemon/gdm.c:484
#, c-format
msgid "deal_with_x_crashes: Trying failsafe X server %s"
msgstr "deal_with_x_crashes: X서버 %s에 안전모드로 시도합니다"

#: daemon/gdm.c:517
msgid "deal_with_x_crashes: Running the XKeepsCrashing script"
msgstr "deal_with_x_crashes: XKeepsCrashing 스크립트를 실행합니다"

#: daemon/gdm.c:532 daemon/gdm.c:592 daemon/gdm.c:606
msgid ""
"I cannot start the X server (your graphical interface).  It is likely that "
"it is not set up correctly.  You will need to log in on a console and rerun "
"the X configuration program.  And then restart GDM."
msgstr ""
"X 서버(그래픽 인터페이스)를 시작할 수 없습니다. 설정을 올바르게 맞추지 않은"
"것 같습니다. 콘솔로 로그인 하여 X설정 프로그램을 다시 실행 하여야 합니다."
"그리고 GDM을 다시 시작 하십시오."

#: daemon/gdm.c:537
msgid ""
"I cannot start the X server (your graphical interface).  It is likely that "
"it is not set up correctly.  Would you like me to try to run the X "
"configuration program?  Note that you will need the root password for this."
msgstr ""
"X 서버(그래픽 인터페이스)를 시작할 수 없습니다. 설정을 올바르게 맞추지 않은"
"것 같습니다. X 설정 프로그램을 실행 하시겠습니까? 실행 하려면 root 비밀번호"
"가 필요합니다."

#: daemon/gdm.c:542
msgid "Please type in the root (privilaged user) password."
msgstr "root(특권 사용자)의 비밀번호를 입력하십시오."

#: daemon/gdm.c:544
msgid "I will now try to restart the X server again."
msgstr "X서버를 다시 시작하려 합니다."

#: daemon/gdm.c:546
msgid ""
"I will disable this X server for now.  Restart GDM when it is configured "
"correctly."
msgstr ""
"지금 X서버를 끕니다. 설정을 올바르게 한 다음 GDM을 다시 시작 하십시오"

#. else {
#. * At this point .... screw the user, we don't know how to
#. * talk to him.  He's on some 'l33t system anyway, so syslog
#. * reading will do him good
#. * }
#: daemon/gdm.c:622
#, c-format
msgid ""
"Failed to start X server several times in a short time period; disabling "
"display %s"
msgstr "짧은 시간에 여러번 X서버 기동 실패; 디스플레이 %s 사용 불가능"

#: daemon/gdm.c:663
#, c-format
msgid ""
"gdm_child_action: Reboot or Halt request when there is no system menu from "
"display %s"
msgstr ""
"gdm_child_action: 디스플레이 %s에서 시스템 메뉴가 없을때 재시작 또는 중지"
"요청"

#: daemon/gdm.c:670
#, c-format
msgid "gdm_child_action: Reboot or Halt request from a non-local display %s"
msgstr ""
"gdm_child_action: 로컬 디스플레이가 아닌 곳 %s 에서 재시작 또는 중지 요청"

#. Bury this display for good
#: daemon/gdm.c:678
#, c-format
msgid "gdm_child_action: Aborting display %s"
msgstr "gdm_child_action: 디스플레이 %s 취소"

#. Reboot machine
#: daemon/gdm.c:684
msgid "gdm_child_action: Master rebooting..."
msgstr "gdm_child_action: 마스터 시스템 재시작..."

#: daemon/gdm.c:691
#, c-format
msgid "gdm_child_action: Reboot failed: %s"
msgstr "gdm_child_action: 시스템 재시작 실패: %s"

#. Halt machine
#: daemon/gdm.c:695
msgid "gdm_child_action: Master halting..."
msgstr "gdm_child_action: 마스터 시스템 종료..."

#: daemon/gdm.c:702
#, c-format
msgid "gdm_child_action: Halt failed: %s"
msgstr "gdm_child_action: 시스템 종료 실패: %s"

#: daemon/gdm.c:784
msgid "Failed to restart self"
msgstr "재시작 실패"

#: daemon/gdm.c:834
msgid "Only root wants to run gdm\n"
msgstr "루트만이 gdm을 실행합니다\n"

#: daemon/gdm.c:857
msgid "gdm already running. Aborting!"
msgstr "gdm은 이미 실행중입니다. 취소!"


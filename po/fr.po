#: daemon/gdm.c:348
#, c-format
msgid "%s: BaseXsession empty, using %s/gdm/Xsession"
msgstr "%s : BaseXsession vide, utilise %s/gdm/Xsession"

#: daemon/gdm.c:389
#, c-format

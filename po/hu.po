#: daemon/gdm.c:114
#, c-format
msgid "gdm_config_parse: No configuration file: %s. Aborting."
msgstr "gdm_config_parse: Nincs %s beállítás fájl. Mûvelet megszakítása."

#: daemon/gdm.c:162
msgid "gdm_config_parse: No greeter specified."
msgstr "gdm_config_parse: Az üdvözlõ nem lett megadva."

#: daemon/gdm.c:165
msgid "gdm_config_parse: No authdir specified."
msgstr "gdm_config_parse: A jogkönyvtár nincs megadva."

#: daemon/gdm.c:171
msgid "gdm_config_parse: No sessions directory specified."
msgstr "gdm_config_parse: A session könyvtár nincs megadva."

#: daemon/gdm.c:183
msgid "gdm_config_parse: Invalid server line in config file. Ignoring!"
msgstr ""
"gdm_config_parse: Érvénytelen szerver sor a beállításokban. Figyelmen kívül "
"hagytam!"


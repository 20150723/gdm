msgstr "gdm_child_action : arrêt du maître..."

#: daemon/gdm.c:762
#, c-format
msgid "gdm_child_action: Halt failed: %s"
msgstr "gdm_child_action : échec de l'arrêt : %s"

#. Suspend machine
#: daemon/gdm.c:766

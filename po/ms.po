#, c-format
msgid "%s: %s is writable by group."
msgstr "%s: %s boleh ditulis oleh kumpulan."

#: daemon/filecheck.c:70
#, c-format
msgid "%s: %s is writable by other."
msgstr "%s: %s boleh ditulis oleh semua."

#: daemon/filecheck.c:84

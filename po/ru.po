#, c-format
msgid "gdm_auth_user_add: Could not lock cookie file %s"
msgstr "gdm_auth_user_add: Не удалось заблокировать файл cookie %s"

#: daemon/auth.c:470
#, c-format
msgid "%s: Could not write cookie"
msgstr "%s: Не удалось создать cookie!"

#: daemon/auth.c:531

#, c-format
msgid "gdm_config_parse: Authdir %s does not exist. Aborting."
msgstr "gdm_config_parse: A %s jogkönyvtár nem létezik. Mûvelet megszakítása."

#: daemon/gdm.c:250
#, c-format
msgid "gdm_config_parse: Authdir %s is not a directory. Aborting."
msgstr "gdm_config_parse: A %s jogkönyvtár nem könyvtár. Mûvelet megszakítása."

#: daemon/gdm.c:253
#, c-format
msgid ""
"gdm_config_parse: Authdir %s is not owned by user %s, group %s. Aborting."
msgstr ""
"gdm_config_parse: A %s jogkönyvtár nem %s felhasználó és %s csoport "
"tulajdonában van. Mûvelet megszakítása."

#: daemon/gdm.c:257
#, c-format
msgid ""
"gdm_config_parse: Authdir %s has wrong permissions. Should be 750. Aborting."
msgstr ""
"gdm_config_parse: A %s jogkönyvtár jogai rosszak. 750 kellene legyen. "
"Mûvelet megszakítása."


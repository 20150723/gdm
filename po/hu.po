#, c-format
msgid "%s: Directory %s does not exist."
msgstr "%s: A(z) %s könyvtár nem létezik."

#: daemon/filecheck.c:57 daemon/filecheck.c:98
#, c-format
msgid "%s: %s is not owned by uid %d."
msgstr "%s: A(z) %s tulajdonosa nem a(z) %d uid."

